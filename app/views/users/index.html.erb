<div class="page-header">
	<h1>User Administration</h1>
</div>
<br>
<div class="row">
	<table class="table table-striped table-hover" id="users_table">
		<thead>
			<tr>
				<th>first</th>
				<th>last</th>
				<th>email</th>
				<th>actions</th>
			</tr>
		</thead>
		<tbody>
			<%= render @users %>
		</tbody>
	</table>
</div>


<script>
$(document).ready(
	function() {
		$('#users_table').DataTable({
			paginationType: 'full_numbers'
		})
	}
	)
</script>