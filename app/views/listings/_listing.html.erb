<a href=<%= book_listing_path(listing.book, listing) %> class="thumbnail text-center"  style="text-decoration:none;" >
	<div class="caption" style="background:#<%= (listing.user == current_user)? "AAAAAA" : "FFFFFF" %>">
		<h2 ><%= show_price(listing.min_price, 0) %></h2>
		<h4><%= show_type(listing) %> </h4>
		<h4><%= show_quality(listing)%></h4>
		<h4><%= show_pickup_option(listing) %></h4>
	</div>
</a>
